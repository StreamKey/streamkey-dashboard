{
  "name": "streamkey-dashboard",
  "version": "0.18.1",
  "license": "MIT",
  "engines": {
    "node": ">=9.6.1"
  },
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "yarn lint && razzle test --env=jsdom",
    "lint": "./node_modules/.bin/standard --parser=babel-eslint | ./node_modules/.bin/snazzy",
    "start:prod": "NODE_ENV=production node build/server.js",
    "db:install": "docker run --name streamkey-postgres -e POSTGRES_PASSWORD=mysecretpassword -d -p 5445:5432 postgres",
    "db:start": "docker start streamkey-postgres",
    "db:rm": "docker stop streamkey-postgres && docker rm streamkey-postgres",
    "workers:fetchData": "yarn babel-node src/workers/scrapeDaily/fetchData --presets es2015,stage-2",
    "workers:createReport": "yarn babel-node src/workers/scrapeDaily/createReport --presets es2015,stage-2",
    "workers:clearLogs": "yarn babel-node src/workers/scrapeDaily/clearLogs --presets es2015,stage-2",
    "workers:createUser": "yarn babel-node src/workers/createUser/index.js --presets es2015,stage-2",
    "workers:sheets": "yarn babel-node src/workers/sheets.js --presets es2015,stage-2"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.1",
    "axios": "^0.19.0",
    "axios-cookiejar-support": "^0.4.2",
    "axios-retry": "^3.1.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^3.0.1",
    "blob": "^0.0.4",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "connect-multiparty": "^2.2.0",
    "connect-pg-simple": "^5.0.0",
    "crypto-js": "^3.1.9-1",
    "csv-stringify": "^4.3.1",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.3",
    "forever": "^0.15.3",
    "glob": "^7.1.3",
    "googleapis": "^34.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "lodash-deep": "^2.0.0",
    "material-ui": "^0.20.2",
    "material-ui-pickers": "^1.0.0-rc.17",
    "mdi-material-ui": "^5.5.0",
    "mdi-svg": "^2.2.43",
    "moment": "^2.22.2",
    "nodemailer": "^4.6.8",
    "numeral": "^2.0.6",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "qs": "^6.5.2",
    "razzle": "^2.4.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-json-view": "^1.19.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-svg": "^6.0.18",
    "react-table": "^6.8.6",
    "redux": "^4.0.1",
    "sequelize": "^5.3.5",
    "sequelize-cli": "^5.1.0",
    "ssh2": "^0.6.1",
    "tough-cookie": "^2.4.3",
    "uuid": "^3.3.2",
    "validator": "^10.8.0",
    "winston": "^3.1.0",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "5.7.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "jest-get-type": "^22.4.3",
    "shipit-cli": "^4.1.1",
    "shipit-deploy": "^4.1.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "node_modules/**"
    ],
    "parser": "babel-eslint"
  }
}
