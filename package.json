{
  "name": "top-sub",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=9.6.1"
  },
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "yarn lint && razzle test --env=jsdom",
    "lint": "./node_modules/.bin/standard --parser=babel-eslint | ./node_modules/.bin/snazzy",
    "start:prod": "NODE_ENV=production ./node_modules/.bin/forever build/server.js",
    "db:start": "docker run --name streamkey-postgres -e POSTGRES_PASSWORD=mysecretpassword -d -p 5445:5432 postgres",
    "db:rm": "docker stop streamkey-postgres && docker rm streamkey-postgres",
    "workers:scrapeDaily": "babel-node src/workers/scrapeDaily/index.js --presets es2015,stage-2",
    "workers:createUser": "babel-node src/workers/createUser/index.js --presets es2015,stage-2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-cookiejar-support": "^0.3.5",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "connect-pg-simple": "^4.2.1",
    "crypto-js": "^3.1.9-1",
    "csvtojson": "^1.1.9",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.42",
    "material-ui-pickers": "^1.0.0-rc.6",
    "mdi-svg": "^2.1.99",
    "moment": "^2.21.0",
    "numeral": "^2.0.6",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "qs": "^6.5.1",
    "razzle": "^0.8.12",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.3.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-svg": "^2.2.15",
    "redux": "^3.7.2",
    "sequelize": "^4.37.3",
    "sequelize-cli": "^4.0.0",
    "tough-cookie": "^2.3.4",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "8.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "jest-get-type": "^22.1.0",
    "shipit-cli": "^4.0.2",
    "shipit-deploy": "^4.0.2",
    "snazzy": "^7.0.0",
    "standard": "^11.0.1"
  },
  "standard": {
    "ignore": [
      "node_modules/**"
    ],
    "parser": "babel-eslint"
  }
}
